
<%- include('partials/header') %>
<main>
    <h1>Top 10 Streaks!</h1>
    <table>
        <tr>
            <th>Rank</th>
            <th>Streak</th>
            <th>Date</th>
        </tr>
        
        <% topLeaderboard.forEach((entry, index) => { %> 
        <tr>
            <td><%= index + 1 %></td> 
            <td><%= entry.streak %></td> 
            <td><%= entry.date.toLocaleString() %></td> 
        </tr>
        <% }) %>
    </table>
    <a href="/">Home</a>
</main>
</body>
</html>